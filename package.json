{
  "name": "stage-2-catalog",
  "version": "1.0.0",
  "description": "stage 2. Catalog project. Angular + Node.js",
  "main": "index.js",
  "scripts": {
    "install:server": "npm install",
    "install:client": "cd client/ && npm install",
    "install:all": "npm run install:server & npm run install:client",
    "dev:server": "nodemon index.js",
    "dev:client": "cd client/ && ng serve",
    "dev:all": "npm run dev:server & npm run dev:client",
    "build:client": "cd client/ && ng build --prod",
    "production": "npm run build:client && pm2 start index.js",
